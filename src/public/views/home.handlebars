<!DOCTYPE html>
<html>
<head>
    <title>Lista de productos</title>
</head>
<body>
    <h1>Lista de Productos</h1>
    <ul>
        {{#each products}}
            <li>{{this.title}} - {{this.price}}</li>
        {{/each}}
    </ul>

    <script src="socket.io/socket.io.js"></script>
    <script>
        let socket = io();

        socket.on('connect', ()=> {
            console.log('Conectado al servidor');
        });
        socket.on('disconnect', ()=> {
            console.log('Me desconecte');
        });
    </script>
</body>
</html>